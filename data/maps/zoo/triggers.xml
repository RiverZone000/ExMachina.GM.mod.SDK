<triggers>
	<trigger Name="Start2" active="0">
		<event eventid="GE_SKIP_CINEMATIC" ObjName="Player1" />
		<event eventid="GE_END_CINEMATIC" ObjName="Player1" />
		<script>
			GameFiltersUse()

			trigger:Deactivate() 
		</script>
	</trigger>

	<trigger Name="rolik" active="0">
		<event timeout="1" eventid="GE_TIME_PERIOD" />
		<script>
println("11111111111111111111111")
--			CinemaFiltersUse()
			TActivate( "Start2" )

			local Plf = GetPlayerVehicle()
			local PlfID = GetPlayerVehicleId()
			local PlfCoor = Plf:GetPosition()
			PlfCoor.y = PlfCoor.y + 13*0.75
			PlfCoor.z = PlfCoor.z + 13*0.75

			Plf:SetCustomControlEnabled( true )
			Plf:SetCustomLinearVelocity( 0 )
			Plf:SetThrottle( 0 )
			Plf:SetCustomControlEnabled( false )

			FlyAround(-5, -0.5, 13, 10, PlfCoor, PlfID, 1, 1 )

			trigger:Deactivate() 
		</script>
	</trigger>


